SET SYSMENU TO
SET SYSMENU AUTOMATIC

DEFINE PAD _20e0k5ab2 OF _MSYSMENU PROMPT "Файл" COLOR SCHEME 3 ;
	KEY ALT+A, "ALT+A"
DEFINE PAD _20e0k5ab3 OF _MSYSMENU PROMPT "Дейстия" COLOR SCHEME 3 ;
	KEY ALT+D, "ALT+D"
DEFINE PAD _20e0k5ab4 OF _MSYSMENU PROMPT "Справочники" COLOR SCHEME 3 ;
	KEY ALT+C, "ALT+C"
DEFINE PAD _20e0k5ab5 OF _MSYSMENU PROMPT "Документы" COLOR SCHEME 3 ;
	KEY ALT+L, "ALT+L"
DEFINE PAD _20e0k5ab6 OF _MSYSMENU PROMPT "Журналы" COLOR SCHEME 3 ;
	KEY ALT+F2, "ALT+F2"
DEFINE PAD _20e0k5ab7 OF _MSYSMENU PROMPT "Отчеты" COLOR SCHEME 3 ;
	KEY ALT+O, "ALT+O"
DEFINE PAD _20e0k5ab8 OF _MSYSMENU PROMPT "Администрирование" COLOR SCHEME 3 ;
	KEY ALT+F, "ALT+F"
DEFINE PAD _20e0k5ab9 OF _MSYSMENU PROMPT "Помощь" COLOR SCHEME 3 ;
	KEY ALT+F1, "ALT+F1"
ON PAD _20e0k5ab2 OF _MSYSMENU ACTIVATE POPUP файл
ON PAD _20e0k5ab3 OF _MSYSMENU ACTIVATE POPUP дейстия
